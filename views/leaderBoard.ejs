<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>Leaderboard</title>
    <link rel="stylesheet" type="text/css" href="/css/stylelogin.css">
    <div class="main">
            <div class="logo">
                <img src="/images/logo.png">
        </div>
  </head>
  <body>
    <br>
    <br>
    
    <h2>Leaderboard</h2><br>
    <div class="login" style="width: 80%;">
    <table style="width: 80%; margin: auto;">

      <tr>
        <th>Points</th>
        <th>Team name</th>
        <th>Members</th>
        <th>Manager</th>
      </tr>
      <% results.forEach(result=>{ %>
        <tr>
            <td><%= typeof result.points !='undefined' ? result.points : 0 %></td>
            <td><%= result.name %></td>
            <td><%= result.members %></td>
            <td><%= result.manager %></td>
        </tr>
    <% }) %>
</table>
</div>
